{
    "name": "awxjs-severe-weather-module",
    "moduleName": "Severe",
    "version": "1.0.1",
    "main": "index.js",
    "author": "Ben Collin",
    "homepage": "https://www.aerisweather.com/support/docs/toolkits/aeris-js-sdk/",
    "license": "MIT",
    "scripts": {
        "develop": "NODE_ENV='development' webpack serve --mode development --env development",
        "build:js": "rm -fr dist; tsc --sourceMap false --outDir dist/src; babel --config-file ./babel-nodejs.config.js dist/src --out-dir dist/src",
        "build:dev": "webpack --mode development --env development",
        "build:prod": "NODE_ENV='production' webpack --mode production --env production",
        "build": "yarn build:js && yarn build:prod && rm dist/index.html",
        "push": "yarn build && git add . && git commit -m 'build: latest compiled code' && git push",
        "dev-server": "yarn develop",
        "lint": "eslint 'src/**/*.{js,ts,tsx}'",
        "lint:fix": "yarn lint -- --fix"
    },
    "husky": {
        "pre-commit": "lint-staged",
        "pre-push": "yarn lint"
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "yarn lint:fix",
            "git add"
        ]
    },
    "files": [
        "/dist",
        "/docs",
        "LICENSE",
        "README.md"
    ],
    "dependencies": {
        "@aerisweather/javascript-sdk": "^1.8.4"
    },
    "devDependencies": {
        "@aerisweather/eslint-plugin": "^1.0.4",
        "@babel/cli": "^7.24.8",
        "@babel/core": "^7.24.9",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.24.7",
        "@babel/preset-env": "^7.25.0",
        "@babel/preset-typescript": "^7.24.7",
        "@typescript-eslint/eslint-plugin": "^7.17.0",
        "@typescript-eslint/parser": "^7.17.0",
        "babel-loader": "^9.1.3",
        "babel-plugin-add-module-exports": "^1.0.4",
        "clean-webpack-plugin": "^4.0.0",
        "dotenv": "^16.4.5",
        "eslint": "^9.8.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-unicorn": "^55.0.0",
        "html-webpack-harddisk-plugin": "^2.0.0",
        "html-webpack-plugin": "^5.6.0",
        "husky": "^9.1.3",
        "lint-staged": "^15.2.7",
        "regenerator-runtime": "^0.14.1",
        "terser-webpack-plugin": "^5.3.10",
        "ts-loader": "^9.5.1",
        "typescript": "^5.5.4",
        "webpack": "^5.93.0",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4"
    },
    "resolutions": {
        "@types/ws": "8.5.3",
        "@types/node": "14.14.37"
    }
}
